{
  "version": 2,
  "functions": {
    "api/index.py": {
      "runtime": "@vercel/python@3.9.0"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/admin/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/media/(.*)",
      "dest": "/media/$1"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "library_seat_booking.production_settings"
  }
}
